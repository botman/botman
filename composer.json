{
    "name": "mpociot/botman",
    "license": "MIT",
    "description": "Create messaging bots in PHP with ease.",
    "keywords": [
        "Bot",
        "BotMan",
        "Slack",
        "Facebook Messenger",
        "Nexmo",
        "Telegram",
        "Skype",
        "Laravel"
    ],
    "homepage": "http://github.com/mpociot/botman",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/http-foundation": "2.8.*|3.0.*|3.1.*|3.2.*",
        "tightenco/collect": "~5.0",
        "opis/closure": "^2.3"
    },
    "require-dev": {
        "codeigniter/framework": "~3.0",
        "illuminate/support": "~5.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "dev-master",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1",
        "doctrine/cache": "^1.6",
        "symfony/cache": "^3.1",
        "mpociot/slack-client": "^0.2.6"
    },
    "suggest": {
        "mpociot/slack-client": "Use the Slack RTM API with BotMan",
        "doctrine/cache": "Use any Doctrine cache driver for cache",
        "symfony/cache": "Use any Symfony cache driver for cache"
    },
    "autoload": {
        "psr-0": {
            "Mpociot\\BotMan": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpociot\\BotMan\\Tests\\": "tests/"
        }
    }
}
